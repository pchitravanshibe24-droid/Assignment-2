#include <iostream>
using namespace std;

int main() {
    string s;
    cout << "Enter string: ";
    cin >> s;
    string res = "";
    for (char ch : s) {
        if (ch!='a' && ch!='e' && ch!='i' && ch!='o' && ch!='u' &&
            ch!='A' && ch!='E' && ch!='I' && ch!='O' && ch!='U')
            res += ch;
    }
    cout << "String after deleting vowels: " << res;
    return 0;
}
